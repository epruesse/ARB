	TAG	"EBI"
	APPEND	"num_bib"

MATCH	"RP *"
	SRT	"RP   *=*:*=[$x]\: *"
	TAG	"EBI"
	APPEND	"nuc_rp"

MATCH	"RC *"
	SRT	"RC   *=*:*=[$x]\: *"
