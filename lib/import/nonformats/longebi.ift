# Global settings:
KEYWIDTH	5
FILETAG		"EBI"

BEGIN	"ID*"

MATCH	"ID *"
	SRT "ID   * *=*1"
	WRITE "name"

MATCH	"ID *"
	SRT "ID   * *=*1"
	TAG	"EBI"
	WRITE "id"

MATCH	"ID *"
	SRT "*;*;*; * BP.=*4"
	TAG	"EBI"
	WRITE "db_nuc"

MATCH	"AC *"
	ACI "extract_words("0123456789",4.0)"
	WRITE "acc"

MATCH	"AC *"
	TAG	"EBI"
	ACI "extract_words("0123456789",4.0)"
	WRITE "db_acc"

MATCH	"SV *"
	TAG	"EBI"
	ACI "extract_words("0123456789",4.0)"
	WRITE "version"

MATCH	"DT *"
	SRT "DT   *=*"
	TAG	"EBI"
	APPEND "date"

MATCH	"DE *"
	SRT	"DE   *=*:;="
	TAG	"EBI"
	APPEND	"description"

MATCH	"KW *"
	SRT	"KW   *=*"
	TAG	"EBI"
	APPEND	"keywd"

MATCH	"OS *"
	SRT "OS   *=*:* * *=*1 *2"
