item:Import using MAUS
itemmeta: M
itemmethod:(cd $ARBHOME/PERL_SCRIPTS/MAUS;perl mausdriver.pl -if=$INPUTFILE -of=OUTPUTFILE \
-exff=$EXCLUDEFILE -exfl=$EXLCUDELIST -kwl=$KWLIST -acl=ACLIST -avl=$AVLIST\
-log=$LOG -cdat=$CDATE -udat=$UDATE -min=$MIN -max=$MAX -spec=$SPEC )

itemhelp:MAUS.help

arg:INPUTFILE
argtype:text
arglabel:Input Sequence File

arg:EXCLUDEFILE
argtype:text
arglabel:File Name for Exclude Accession Numbers

arg:EXCLUDELIST
argtype:text
arglabel:List of Excluded Accession Numbers

arg:KWLIST
argtype:text
arglabel:List Of Key Words

arg:ACLIST
argtype:text
arglabel:List Of Accession Numbers

arg:AVLIST
argtype:text
arglabel:List Of Version Numbers

arg:LOG
argtype:text
arglabel:Name of Log File

arg:CDATE
argtype:text
arglabel:Date of Creation

arg:UDATE
argtype:text
arglabel:Date of Last Update

arg:MIN
argtype:text
arglabel:Minimal Sequence Length

arg:MAX
argtype:text
arglabel:Maximal Sequence Length

arg:SPEC
argtype:text
arglabel:Species Name Compoments

out:OUTPUTFILE
outformat:genbank

