MATCH           "               gene: *"
                TAG             "WL"
                SRT             "gene\: *=*1"
                WRITE           "gene"

MATCH           "               primer: *"
                TAG             "WL"
                SRT             "primer\: *=*1"
                WRITE           "primer"

SEQUENCEAFTER   ".."
SEQUENCEEND     "//"

END             "//"

CREATE_ACC_FROM_SEQUENCE
