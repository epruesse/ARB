#!/usr/bin/perl -w

use probe_server;
use strict;

my $clientfile = probe_server::clientfilename();

if (-f $clientfile) {
  probe_server::print_download_header('application/jar', probe_server::clientbasename());
  system("cat $clientfile");
}
else {
  probe_server::print_header();
  probe_server::print_critical_error("no such file:'$clientfile'");
}

