#define RM_LOCAL_DIR(in1)       cd ~/.arb_tmp/$$ ; STD_REMOVE(in1); cd .. ; rmdir $$

#define GEN_INFILE(in1)         GOTO_LOCAL_DIR ; arb_convert_aln -GenBank in1 -phylip infile
#define RM_LOCAL_FILES(in1)     RM_LOCAL_DIR(in1)

