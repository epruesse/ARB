argtype:slider
argmin:1
argmax:100
argvalue:10

arg:REPORT
argtype:chooser
arglabel:View assembly report?
argchoice:No:
argchoice:Yes:${ARB_TEXTEDIT:-textedit} in1.rpt&

 
in:in1
informat:flat
insave:

