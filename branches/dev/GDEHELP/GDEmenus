argvalue:1
argchoice:Yes:yes
argchoice:No:no

arg:SCORE
argtype:slider
arglabel:Number of High Scoring Pairs Displayed?
argmin:1
argmax:250
argvalue:5

in:in1
informat:genbank
insave:

item:Blastp (Protein)
