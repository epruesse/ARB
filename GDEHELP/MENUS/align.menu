
item:ClustalW automatic
itemmeta: W
itemmethod:(tr '"%//' '>' <in1 >clus_in; \
            RUN_IN_WINDOW((clustalw -output=PIR -infile=clus_in -align -interactive ; \
                           cat clus_in.pir | sed "s/^>DL;/#/g" | sed "s/^>P1;/%/g" | sed "s/^\*$//g" >out1; )); \
            /bin/rm -f clus_in* in1)

itemhelp:clustalw.help


in:in1
informat:flat
insave:

out:out1
outformat:flat

